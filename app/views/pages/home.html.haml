#home
  #eventsTab
    = image_tag("/images/upcomingtab.gif")
  .upcoming
    - unless @upcoming_events.count < 1
      %ul.event_list
        - @upcoming_events.each do |event|
          %li.title
            - if event.venues.first
              = link_to "#{event.title} @ #{event.venues[0].venue_name}", event
            - else
              = link_to "#{event.title}", event
  .promotions
    - @promotions.each do |promo|
      = render(:partial => 'promotions/promo', :locals => {:promotion => promo})

