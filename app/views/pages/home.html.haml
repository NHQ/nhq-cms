- content_for :head do
  = javascript_include_tag "date"

%h1.home 500 Clown
#home
  .description Life is worth the risk.
  .upcoming
    %h3 Upcoming events:
    %ul.upcoming_events
      - @upcoming_events.each do |event|
        %li.title
          - if event.venues.first
            = link_to "#{event.title} at #{event.venues[0].venue_name}", event
          - else
            = link_to "#{event.title}", event

  %h3.home Feed:
  .facebookfeed.g560

- content_for :jstemplates do
  jQuery(function() { app.getFeed(); });
